// Online C++ compiler to run C++ program online
#include <iostream>
#include <cmath>
using namespace std;
int digMen(int n){
    int d, dm=n%10;
    int aux=n;
    while(aux>0){
        d=aux%10;
        aux=aux/10;
        if(d<dm){
            dm=d;
        }
    }
    return dm;
}
int digMay(int n){
    int d, dm=n%10;
    int aux=n;
    while(aux>0){
        d=aux%10;
        aux=aux/10;
        if(d>dm){
            dm=d;
        }
    }
    return dm;
}
int frecuencia(int n,int dig){
    int aux=n;
    int c=0;
    int d=0;
    while(aux>0){
        d=aux%10;
        aux=aux/10;
        if(d==dig){
            c++;
        }
    }
    return c;
}

//n=48343432
int digMasRepe(int n){
    int aux=n;int d;
    int dmr=n%10; //digito mas repetido dmr=2
    int fd=frecuencia(n,dmr);  //frecuencia del digito  df=1
    while(aux>0){
        d=aux%10;
        aux=aux/10;
       // cout<<"Dig: "<<d<<" rep:"<<frecuencia(n,d)<<endl;
        int f=frecuencia(n,d);
        if(f>fd){
            dmr=d;
            fd=f;
        }
    }
    return dmr;
}

void ordenarNumero(int& n){
    int d,nn=0,aux=0;
   // for(int i=1;i<10;i++){
    for(int i=9;i>0;i--){
        aux=n;
        while(aux>0){
            d=aux%10;
            aux=aux/10;
            if(i==d){
                nn=nn*10+d;
            }
        }
    }
    n=nn;
}

//invertir  un numero  nn = nn * 10 + d
 
 void invertir(int& n){
     int d=0,nn=0;
     while(n>0){
         d=n%10;
         n=n/10;
         nn = nn * 10 + d;
         //cout<<"nn= "<<nn<<endl;
     }
     n=nn;
 }


//mantener el orden de un numero
//e=0
// nn = nn + d * 10 e
void mantenerOrden(int& n){
    int d,e=0,nn=0;
    while(n>0){
        d=n%10;
        n=n/10;
        nn = nn + d * pow(10,e);
        //cout<<"nn= "<<nn<<" e= "<<e<<endl;
         e++;
    }
    n=nn;
}


int cantDig(int n){
    int c=0;
    int aux=n;
    while(aux>0){
        aux=aux/10;
        c++;
    }
    return c;
}

//encontrar el numero dentro de otro numero
// n= 12345678  n2=67
bool existeNumDentroDeOtroNume(int n,int n2){
    //cuantos digitos tiene n2
    int cd=cantDig(n2);
    int d=0;
    int aux=n;
    while(aux>0){
        d=aux%(int)pow(10,cd);
        aux=aux/10;
        if(d==n2){
            return true;
        }
    }
    return false;
}


void mostrarNdDigitos(int n,int nd){
    int d=0;
    while(n>0){
        d=n% (int)pow(10,nd);
       // n=n/(int)pow(10,nd);
         n=n/10;
        cout<<"d= "<<d<<endl;
    }
}


//poker
bool poker(int n){
    return (
       ( frecuencia(n,digMay(n)) ==1 &&frecuencia(n,digMen(n))==4 ) ||
       ( frecuencia(n,digMay(n)) ==4 &&frecuencia(n,digMen(n))==1 )
       
     //1 caso dmay=1 y dmen=4    
    );
}
//full
bool full(int n){
    return (
         ( frecuencia(n,digMay(n)) ==2 && frecuencia(n,digMen(n))==3 ) 
       ||
         ( frecuencia(n,digMay(n)) ==3 &&frecuencia(n,digMen(n))==2 )
       
     //1 caso dmay=1 y dmen=4    
    );
}
//trica
//2 pares 
bool son2Pares(int n){
    int d,c=0,aux=n;
    while(aux>0){
        d=aux%10;
        aux=aux/10;
        if(frecuencia(n,d)==2){
            c++;
        }
    }
    return (c==4);
}

bool esEscalera(int n){
  ordenarNumero(n);
  cout<<n<<endl; //n= 98732
  int d,aux=n,dr=aux%10;

  while(aux>0){
      d=aux%10; 
      aux=aux/10; 
      //cout<<dr<<" != "<<d<<endl;
      if(dr!=d){ 
         return false; 
      }
      dr++;
      //cout<<"d= "<<d<<" aux= "<<aux<< " dr= "<<dr<<endl;
  }
  return true;
}


void eliminarTodosLosDigitos(int& n, int dig){
    int d,e=0,nn=0;
    while(n>0){
        d=n%10;
        n=n/10;
        if(d!=dig){
            nn=nn+d* pow(10,e);
            e++;
        }
    }
    n=nn;
}

void eliminarNumeroDentroDeOtro(int& n,int n2){
    int d=0,dr=0,nn=0,e=0,cd=cantDig(n2);
    while(n>0){
        d=n% (int)pow(10,cd);
        if(d==n2){
            n=n/pow(10,cd);
        }else{
            dr=n%10;
            n=n/10;
            nn=nn + dr *pow(10,e);
            e++;
        }
    }
    n=nn;
    
    
}
 




int main() {
    int n=213452;
    cout<<"n= "<<n<<" n2=34"<<endl;
    eliminarNumeroDentroDeOtro(n,34);
   // eliminarTodosLosDigitos(n,3);
    cout<<"n= "<<n<<endl;
   // cout<<esEscalera(n);
   // cout<<poker(n);
// cout<<frecuencia(n,2);
 // cout<<"Dig mas rep: "<<digMasRepe(n)<<endl<<endl;
  //ordenarNumero(n);
//  invertir(n);
//mantenerOrden(n);
//mostrarNdDigitos(n,3);
//int n2=234;
//cout<<"n2= "<<n2<<" "<<existeNumDentroDeOtroNume(n,n2);
 // cout<<" n= "<<n;
    return 0;
    
    
    
    
    
}