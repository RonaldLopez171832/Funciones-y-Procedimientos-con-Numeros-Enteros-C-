int unir3numeroAscV3(int n1,int n2,int n3 ){
int nn = 0;
int caseValue = 0;

if(n1 > n2 && n1 > n3) {
    if(n2 > n3) {
        caseValue = 1; // n1 > n2 > n3
    } else {
        caseValue = 2; // n1 > n3 > n2
    }
} else if(n2 > n1 && n2 > n3) {
    if(n1 > n3) {
        caseValue = 3; // n2 > n1 > n3
    } else {
        caseValue = 4; // n2 > n3 > n1
    }
} else {
    if(n1 > n2) {
        caseValue = 5; // n3 > n1 > n2
    } else {
        caseValue = 6; // n3 > n2 > n1
    }
}

switch(caseValue) {
    case 1:
        return unir3Numeros(n3, n2, n1); // n1 > n2 > n3
    case 2:
        return unir3Numeros(n2, n3, n1); // n1 > n3 > n2
    case 3:
        return unir3Numeros(n3, n1, n2); // n2 > n1 > n3
    case 4:
        return unir3Numeros(n1, n3, n2); // n2 > n3 > n1
    case 5:
        return unir3Numeros(n2, n1, n3); // n3 > n1 > n2
    case 6:
        return unir3Numeros(n1, n2, n3); // n3 > n2 > n1
    default:
        return -1; // No matching case
}

}


//Ejemplo: N = 213452,
//NA₁ = 34,
//NA₂ = 10 → Resultado = 211052

void ejercicioDificilEjercicio1(int& n, int na1, int na2){
    int d,nn=0,e=0;
    int cd=cantDig(na1);
    while(n>0){
        d=n % (int)pow(10,cd);
        n=n/ pow(10,cd);
        if(d==na1)
            nn=nn+na2 *pow(10,e);
        else
            nn=nn+d*pow(10,e);
        e=e+cd;
    }
    n=nn;
}


//Ejemplo: N = 213452,
//na1 = 45
//na2 = 99
//n= 213992
void ejercicioDificilEjercicio1V2(int& n, int na1, int na2){
    int d,nn=0,e=0;
    int cd=cantDig(na1);
    //n=213452
    while(n>0){
        d=n % (int)pow(10,cd); //D=52
        if(d==na1){ //52 == 45
            n=n/ pow(10,cd);  //N=21345
            nn=nn+na2 *pow(10,e);
             e=e+cd;
        }else{
            n=n/ 10;  //N=21345
            d=d%10; //2
             nn=nn+d*pow(10,e);  // 2
             e++;
        }
    }
    n=nn;
}


//Ejemplo: N = 213452,
//na1 = 45
//na2 = 999
//n= 2139992

void ejercicioDificilEjercicio1V3(int& n, int na1, int na2){
    int d,nn=0,e=0;
    int cd=cantDig(na1);
    int cdna2=cantDig(na2);
    //n=213452
    while(n>0){
        d=n % (int)pow(10,cd); //D=13
        if(d==na1){ //13 == 45
            n=n/ pow(10,cd);  //213
            nn=nn+na2 *pow(10,e); //9992
            e=e+cdna2; //e =3
           // cout<<"nn= "<<nn<<" e= "<<e<<endl;
        }else{
            n=n/ 10;  //N=21
            d=d%10; //3
            nn=nn+d*pow(10,e);  // 39992
            e++;
           // cout<<"nn= "<<nn<<" e= "<<e<<endl;
        }
    }
    n=nn;
}

//N = 1234567
//F = 1/5! − 1/3! + 1/2! − 1/1!

int factorial(int n){
    int f=1;
    for (int i=1;i<=n;i++){
        f=f*i;
    }
    return f;
}

bool verifFibo(int n){
    int a=-1,b=1,f=0;
    while(f<=n){
        f=a+b;
        a=b;
        b=f;
        if(f==n)
          return true;
    }
    return false;
}
//N = 1234567
//F = 1/5! − 1/3! + 1/2! − 1/1!
double ejercicioDificilEjer7(int n){
    double f=0;
    int d=0;
    int na=n;
    bool bandera=true;
    cout<<"f = ";
    while(na>0){
        d=na%10;
        na=na/10;
        if(verifFibo(d)){
            if(bandera){
               f=f + (1/ (double)factorial(d));
               cout<< "+ 1/"<<d<<"! ";
            }else{
               f=f - (1/ (double)factorial(d));
               cout<< "- 1/"<<d<<"! ";
            }
            bandera=!bandera;  
           // cout<<"F= "<<f<<endl;
        }
    }
    cout<<" = ";
    return f;
}


bool ejercicioDificilEjer8(int n, int nd){
    int d;int na=n;
    while (na>0){
        d=na % (int)pow(10,nd);
        na=na/pow(10,nd);
        if(!verifFibo(d))return false;
    }
   return true; 
}

bool ejercicioDificilEjer9(int n, int nd){
    int d;int na=n; //n= 12 34 56 78
    int dr=na % (int)pow(10,nd); //78
    na=na/pow(10,nd); //n=12 34 56
    while(na>0){
        d=na % (int)pow(10,nd);
         na=na/pow(10,nd);
         if(dr>=d) dr=d;
         else return false;
    }
    return true;
}

int ejercicioDificilEjer6(int n, int base){
  int residuo,na=n,nn=0,e=0;
  while(na>0){
      residuo=na%base;
      // cout<< na<<"/"<<base<<" r="<<residuo<<endl;
      na=na/base;
      //cout<< na<<"/"<<base<<" na="<<na<<endl;
      nn=nn+residuo*pow(10,e);
      //cout<< "nn= "<<nn<<endl;
      e++;
  }
  return nn;  
}
    
    

