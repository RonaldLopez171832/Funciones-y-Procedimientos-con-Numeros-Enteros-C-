// Online C++ compiler to run C++ program online
#include <iostream>
#include <cmath>
using namespace std;
int digMen(int n){
    int d, dm=n%10;
    int aux=n;
    while(aux>0){
        d=aux%10;
        aux=aux/10;
        if(d<dm){
            dm=d;
        }
    }
    return dm;
}
int digMay(int n){
    int d, dm=n%10;
    int aux=n;
    while(aux>0){
        d=aux%10;
        aux=aux/10;
        if(d>dm){
            dm=d;
        }
    }
    return dm;
}
int frecuencia(int n,int dig){
    int aux=n;
    int c=0;
    int d=0;
    while(aux>0){
        d=aux%10;
        aux=aux/10;
        if(d==dig){
            c++;
        }
    }
    return c;
}

//n=48343432
int digMasRepe(int n){
    int aux=n;int d;
    int dmr=n%10; //digito mas repetido dmr=2
    int fd=frecuencia(n,dmr);  //frecuencia del digito  df=1
    while(aux>0){
        d=aux%10;
        aux=aux/10;
       // cout<<"Dig: "<<d<<" rep:"<<frecuencia(n,d)<<endl;
        int f=frecuencia(n,d);
        if(f>fd){
            dmr=d;
            fd=f;
        }
    }
    return dmr;
}

void ordenarNumero(int& n){
    int d,nn=0,aux=0;
   // for(int i=1;i<10;i++){
    for(int i=9;i>0;i--){
        aux=n;
        while(aux>0){
            d=aux%10;
            aux=aux/10;
            if(i==d){
                nn=nn*10+d;
            }
        }
    }
    n=nn;
}

//invertir  un numero  nn = nn * 10 + d
 
 void invertir(int& n){
     int d=0,nn=0;
     while(n>0){
         d=n%10;
         n=n/10;
         nn = nn * 10 + d;
         //cout<<"nn= "<<nn<<endl;
     }
     n=nn;
 }


//mantener el orden de un numero
//e=0
// nn = nn + d * 10 e
void mantenerOrden(int& n){
    int d,e=0,nn=0;
    while(n>0){
        d=n%10;
        n=n/10;
        nn = nn + d * pow(10,e);
        //cout<<"nn= "<<nn<<" e= "<<e<<endl;
         e++;
    }
    n=nn;
}


int cantDig(int n){
    int c=0;
    int aux=n;
    while(aux>0){
        aux=aux/10;
        c++;
    }
    return c;
}

//encontrar el numero dentro de otro numero
// n= 12345678  n2=67
bool existeNumDentroDeOtroNume(int n,int n2){
    //cuantos digitos tiene n2
    int cd=cantDig(n2);
    int d=0;
    int aux=n;
    while(aux>0){
        d=aux%(int)pow(10,cd);
        aux=aux/10;
        if(d==n2){
            return true;
        }
    }
    return false;
}


void mostrarNdDigitos(int n,int nd){
    int d=0;
    while(n>0){
        d=n% (int)pow(10,nd);
       // n=n/(int)pow(10,nd);
         n=n/10;
        cout<<"d= "<<d<<endl;
    }
}


//poker
bool poker(int n){
    return (
       ( frecuencia(n,digMay(n)) ==1 &&frecuencia(n,digMen(n))==4 ) ||
       ( frecuencia(n,digMay(n)) ==4 &&frecuencia(n,digMen(n))==1 )
       
     //1 caso dmay=1 y dmen=4    
    );
}
//full
bool full(int n){
    return (
         ( frecuencia(n,digMay(n)) ==2 && frecuencia(n,digMen(n))==3 ) 
       ||
         ( frecuencia(n,digMay(n)) ==3 &&frecuencia(n,digMen(n))==2 )
       
     //1 caso dmay=1 y dmen=4    
    );
}
//trica
//2 pares 
bool son2Pares(int n){
    int d,c=0,aux=n;
    while(aux>0){
        d=aux%10;
        aux=aux/10;
        if(frecuencia(n,d)==2){
            c++;
        }
    }
    return (c==4);
}

bool esEscalera(int n);
void eliminarTodosLosDigitos(int& n, int dig);
void eliminarNumeroDentroDeOtro(int& n,int n2);
//Ejemplo: N = 12345 → Resultado = 51234
//sobrecarga de funciones
void rotarDerecha(int& n){
  int d=n%10; //sacar el digito 5
  n=n/10;     //corto el digito 5 n=1234
  int cd=cantDig(n);
  n=d*pow(10,cd)+n;
}

void rotarDerecha(int& n, int cant){
    for(int i=1;i<=cant;i++){
        rotarDerecha(n);
    }
}
//Ejemplo: N = 12345 → Resultado = 23451
void rotarIzquierda(int& n){
 int cd= cantDig(n); //cant digitos del numero
  int d= n / pow(10, cd-1) ;    //obtener el 1
    n= n%(int)pow(10,cd-1); //n= 2345
    n= n*10 + d;
}

void rotarIzquierda(int& n, int cant){
    for(int i=1;i<=cant;i++){
        rotarIzquierda(n);
    }
}
 bool verifPrimo(int n){
     int c=0;
     for (int i=1;i<=n;i++)
         if(n%i==0) c++;
     return (c==2);
 }
void segmentarPrimoNoPrimo(int& n){
    int nn=0,d=0,aux=n;
    //primero sacamos digitos primos
    while(aux>0){
        d=aux%10;
        aux=aux/10;
        if(verifPrimo(d)){
            nn=nn*10+d;
        }
    }
    aux=n; //recupera el valor de n
     while(aux>0){
        d=aux%10;
        aux=aux/10;
        if(!verifPrimo(d)){
            nn=nn*10+d;
        }
    }
    n=nn;
}

void segmentarPrimoNoPrimo(int& n,int cd){
    int nn=0,d=0,aux=n;
    //primero sacamos digitos primos
    while(aux>0){
        d=aux%(int)pow(10,cd);
        aux=aux/pow(10,cd);
        if(verifPrimo(d)){
            nn=nn*pow(10,cd)+d;
        }
    }
    aux=n; //recupera el valor de n
     while(aux>0){
        d=aux%(int)pow(10,cd);
        aux=aux/pow(10,cd);
        if(!verifPrimo(d)){
            nn=nn*pow(10,cd)+d;
        }
    }
    n=nn;
}

void segmentarPrimoNoPrimoV2(int& n){
    int np=0,nnp=0,d=0,aux=n,cdnp=0;
    //primero sacamos digitos primos
    while(aux>0){
        d=aux%10;
        aux=aux/10;
        if(verifPrimo(d))
            np=np*10+d;
        else{
            nnp=nnp*10+d;
            cdnp++;
        }
    }
    n=np*pow(10,cdnp)+nnp;
}


void intercalarPrimoNoprimo(int& n){
    int np=n,nnp=n,nn=0,d=0;
    bool encontreP=false;
    bool encontreNP=false;
    while(np>0||nnp>0){
        encontreP=false;
        while(np>0&&encontreP==false){
          d=np%10;
          np=np/10;
          if(verifPrimo(d)){
            nn=nn*10+d;
            encontreP=true;
          }
        }
        encontreNP=false;
        while(nnp>0&&encontreNP==false){
          d=nnp%10;
          nnp=nnp/10;
          if(!verifPrimo(d)){
            nn=nn*10+d;
            encontreNP=true;
          }
        }
    }
    n=nn;
}

bool verifCapicua(int n){
    int na=n;   //na=numero auxiliar
    invertir(na);
    return na==n;
}

bool verifCapicua2(int n){
    int na=n;
    int d=0,nn=0;
     while(n>0){
         d=n%10;
         n=n/10;
         nn = nn * 10 + d;
         //cout<<"nn= "<<nn<<endl;
     }
    return na==nn;
}

//n1=10 n2=888 => nn=10888
int unir2Numeros(int n1,int n2){
    int cd=cantDig(n2);
    return  n1* pow(10,cd)+n2;
}


int unir3Numeros(int n1,int n2,int n3){
    int nn=0;
    int cd=cantDig(n2);
    nn= n1* pow(10,cd)+n2;
    cd=cantDig(n3);
    nn= nn* pow(10,cd)+n3;
    return nn;
}


// N₁ = 123, N₂ = 34, N₃ = 7 → Resultado = 734123
int unir3numeroAsc(int n1,int n2,int n3 ){
    int nn=0;
    if(n1>n2 && n1>n3){// N₁ = 123, N₂ = 34, N₃ = 7 → Resultado = 734123
        if(n2>n3){
           nn=unir2Numeros(n2,n1); //34123
           nn=unir2Numeros(n3,nn); //734123
        }else{
            nn=unir2Numeros(n3,n1); //34123
            nn=unir2Numeros(n2,nn); //734123
        }
    }else if(n2>n1 && n2>n3){// N₁ = 34, N₂ = 123, N₃ = 7 → Resultado = 734123
       if(n1>n3){
           nn=unir2Numeros(n1,n2); //
           nn=unir2Numeros(n3,nn); //
        }else{
            nn=unir2Numeros(n3,n2); //
            nn=unir2Numeros(n1,nn); //
        }
    }else if(n3>n1 && n3>n2){// N₁ = 34, N₂ = 7, N₃ = 123 → Resultado = 734123
       if(n1>n2){
           nn=unir2Numeros(n1,n3); //
           nn=unir2Numeros(n2,nn); //
        }else{
            nn=unir2Numeros(n2,n3); //
            nn=unir2Numeros(n1,nn); //
        }
    }
    
    return nn;  
}

