void eliminarTodosLosDigitos(int& n, int dig){
    int d,e=0,nn=0;
    while(n>0){
        d=n%10;
        n=n/10;
        if(d!=dig){
            nn=nn+d* pow(10,e);
            e++;
        }
    }
    n=nn;
}

void eliminarNumeroDentroDeOtro(int& n,int n2){
    int d=0,dr=0,nn=0,e=0,cd=cantDig(n2);
    while(n>0){
        d=n% (int)pow(10,cd);
        if(d==n2){
            n=n/pow(10,cd);
        }else{
            dr=n%10;
            n=n/10;
            nn=nn + dr *pow(10,e);
            e++;
        }
    }
    n=nn;
    
    
}
bool esEscalera(int n){
  ordenarNumero(n);
  cout<<n<<endl; //n= 98732
  int d,aux=n,dr=aux%10;

  while(aux>0){
      d=aux%10; 
      aux=aux/10; 
      //cout<<dr<<" != "<<d<<endl;
      if(dr!=d){ 
         return false; 
      }
      dr++;
      //cout<<"d= "<<d<<" aux= "<<aux<< " dr= "<<dr<<endl;
  }
  return true;
}